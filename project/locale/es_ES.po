#. A tab within the game's options menu containing gameplay-related settings.
msgid "options_gameplay"
msgstr ""

#. A tab within the game's options menu containing sound-related settings.
msgid "options_sound"
msgstr ""

#. Used as a category name for a group of settings related to volume.
msgid "options_sound_volume"
msgstr ""


#. The title of the game, located on the main menu.
msgid "main_game_title"
msgstr ""

#. A "play game" button, located on the main menu.
msgid "main_play"
msgstr "Play"

#. A button to open the options menu, located on the main menu.
msgid "main_options"
msgstr "Options"

#. A button to open the game's recent changes, located on the main menu.
msgid "main_patch_notes"
msgstr "Patch Notes"

#. A button to open the game's credits, located on the main menu.
msgid "main_credits"
msgstr "Credits"

#. A button to quit/exit the game, located on the main menu.
msgid "main_quit"
msgstr "Quit"

#. Displayed to the player on a loading screen and indicates the game is loading.
msgid "loading_in_progress"
msgstr "Loading..."

#. Instructs the player to select which save slot to use.
msgid "save_slots_choose"
msgstr "Choose a save slot â€” progress will be saved automatically"

#. A label for the timestamp of when the particular save was last updated.
msgid "save_slots_last_updated"
msgstr "Last updated:"

#. A label displayed when a save slot is broken/unusable.
msgid "save_slots_broken"
msgstr "Broken"

#. A label displayed when a save slot is empty/available/has no save data.
msgid "save_slots_empty"
msgstr "Empty"

#. A button which the player can press to delete save data for a specific slot.
msgid "save_slots_delete"
msgstr "Delete"

#. A tab within the game's options menu containing gameplay-related settings.
msgid "options_gameplay"
msgstr "Gameplay"

#. A tab within the game's options menu containing sound-related settings.
msgid "options_sound"
msgstr "Sound"

#. Used as a category name for a group of settings related to volume.
msgid "options_sound_volume"
msgstr "Volume"

#. Describes the global volume setting within the options menu.
msgid "options_sound_volume_master"
msgstr "Global"

#. Describes the sound effects volume setting within the options menu.
msgid "options_sound_volume_sound_effects"
msgstr "Sound Effects"

#. Describes the musics volume setting within the options menu.
msgid "options_sound_volume_music"
msgstr "Music"

#. Describes the spoken word volume setting within the options menu.
msgid "options_sound_volume_voice"
msgstr "Voice"

#. Used as a category name for a group of settings related to audio output.
msgid "options_sound_audio"
msgstr "Audio"

#. Describes the setting which changes the specific audio output device used by the game.
msgid "options_sound_audio_device"
msgstr "Audio output device"

#. Describes the setting which causes sound to be muted when the game is unfocused (i.e. when the user "tabs out" of the game).
msgid "options_sound_audio_bg_mute"
msgstr "Mute in background"

#. A tab within the game's options menu containing settings for interacting with the game.
#. This menu includes things like text size, changing the game's display language, etc.
msgid "options_interface"
msgstr "Interface"

#. Used as a category name for a group of settings related to text display.
msgid "options_interface_text"
msgstr "Text"

#. Describes the setting which changes the size of the text.
msgid "options_interface_text_scaling"
msgstr "Scale text size"

#. A tab within the game's options menu containing graphics/video-related settings.
msgid "options_display"
msgstr "Display"

#. Used as a category name for a group of settings related to graphics.
msgid "options_display_video"
msgstr "Video"

#. Describes the setting which toggles the size of the game between "windowed" and "fullscreen".
msgid "options_display_video_fullscreen"
msgstr "Fullscreen"

#. Describes the setting which removes the border around the game application window.
msgid "options_display_video_borderless"
msgstr "Borderless"

#. Describes the setting which adjusts the maximum number of frames drawn per second.
msgid "options_display_video_fps_limit"
msgstr "Max frame limit"

#. Describes the setting which adjusts whether the game's frames drawn per second are capped to the monitor's refresh rate.
msgid "options_display_video_vsync"
msgstr "VSync"

#. A tab within the game's options menu containing controls/input-related settings.
msgid "options_controls"
msgstr "Controls"

#. Used as a category name for a group of non-specific settings.
msgid "options_controls_general"
msgstr "General"

#. Describes the setting which disables controller vibration feedback.
msgid "options_controls_disable_vibration"
msgstr "Disable vibration"

#. Describes the setting which controls how intense controller vibration feedback is.
msgid "options_controls_vibration_strength"
msgstr "Vibration strength"

#. Used as a category name for a group of settings related to Steam Input.
#. Steam Input is a software library/interface for the Steam game distribution platform.
msgid "options_controls_steam"
msgstr "Steam Input"

#. Describes a setting which adjusts the action-to-button bindings for a game controller.
msgid "options_controls_steam_configurator"
msgstr "Controller layout"

#. Used as a category name for a group of input actions related to menu navigation.
msgid "options_controls_menu"
msgstr "Menus"

#. An input action that confirms the current selection.
msgctxt "actions_menu"
msgid "ui_accept"
msgstr "Accept"

#. An input action that navigates (through a menu) back to the previous selected element.
msgctxt "actions_menu"
msgid "ui_back"
msgstr "Back"

#. An input action used to navigate left within a game menu.
msgctxt "actions_menu"
msgid "ui_left"
msgstr "Left"

#. An input action used to navigate right within a game menu.
msgctxt "actions_menu"
msgid "ui_right"
msgstr "Right"

#. An input action used to navigate up within a game menu.
msgctxt "actions_menu"
msgid "ui_up"
msgstr "Up"

#. An input action used to navigate down within a game menu.
msgctxt "actions_menu"
msgid "ui_down"
msgstr "Down"

#. An input action used to navigate to the next item within a game menu.
msgctxt "actions_menu"
msgid "ui_focus_next"
msgstr "Next"

#. An input action used to navigate to the previous item within a game menu.
msgctxt "actions_menu"
msgid "ui_focus_prev"
msgstr "Previous"

#. An input action used to cancel the current selection/leave the current menu page.
msgctxt "actions_menu"
msgid "ui_cancel"
msgstr "Cancel"

#. Used as a category name for a group of input actions related to changing tabs in a tabbed menu.
msgid "options_controls_menu_tabbed"
msgstr "Menu tabs"

#. An input action that moves to the next tab/page within a game menu.
msgctxt "actions_menu_tabbed"
msgid "ui_tab_next"
msgstr "Next tab"

#. An input action that moves to the previous tab/page within a game menu.
msgctxt "actions_menu_tabbed"
msgid "ui_tab_prev"
msgstr "Previous tab"

#. Used as a category name for a group of input actions related to changing which game actions are bound to a controller input button.
msgid "options_controls_menu_binding"
msgstr "Input binding"

#. An input action that cancels the "rebinding" operation.
msgctxt "actions_menu_binding"
msgid "ui_binding_stop"
msgstr "Cancel binding"
